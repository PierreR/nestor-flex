CREATE MEMORY TABLE PROGRAM(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,CREATIONDATE TIMESTAMP,MODIFIEDDATE TIMESTAMP,NAME VARCHAR(255),BUREAUDESIGNATIONDATE TIMESTAMP,CONTRACTTYPE INTEGER,COUNCILDATE TIMESTAMP,GRANTDATE TIMESTAMP,MANAGERDESIGNATIONDATE TIMESTAMP,MANAGERNAME VARCHAR(255),NOTIFICATIONDATE TIMESTAMP,RECIPIENT INTEGER,BUREAU_ID INTEGER,CONSTRAINT FK50C7B664CF658005 FOREIGN KEY(BUREAU_ID) REFERENCES BUREAU(ID))
CREATE MEMORY TABLE PROGRAM$PLANNING(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,DATE TIMESTAMP,DESCRIPTION VARCHAR(255),PROGRAMID_ID INTEGER,CONSTRAINT FK8F1BA1BD3460F614 FOREIGN KEY(PROGRAMID_ID) REFERENCES PROGRAM(ID))
