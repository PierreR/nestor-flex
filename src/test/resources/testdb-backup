CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE MUNICIPALITY(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NAME VARCHAR(255),POSTALCODE VARCHAR(255))
CREATE MEMORY TABLE PROGRAM(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,BUREAUDESIGNATIONDATE TIMESTAMP,BUREAUNAME VARCHAR(255),CONTRACTTYPE INTEGER,COUNCILDATE TIMESTAMP,GRANTDATE TIMESTAMP,MANAGERDESIGNATIONDATE TIMESTAMP,MANAGERNAME VARCHAR(255),NAME VARCHAR(255),NOTIFICATIONDATE TIMESTAMP,REFERENCE VARCHAR(255),MUNICIPALITY_ID INTEGER,CONSTRAINT FK185AAA64F5EE9485 FOREIGN KEY(MUNICIPALITY_ID) REFERENCES MUNICIPALITY(ID))
CREATE MEMORY TABLE PROGRAM_PLANNING(PROGRAM_ID INTEGER NOT NULL,PLANNING_ID INTEGER NOT NULL,PRIMARY KEY(PROGRAM_ID,PLANNING_ID),CONSTRAINT SYS_CT_81 UNIQUE(PLANNING_ID),CONSTRAINT FK5760C2F8E9E8AC0F FOREIGN KEY(PROGRAM_ID) REFERENCES PROGRAM(ID))
CREATE MEMORY TABLE COMPANY(COMPANYID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ADDRESS VARCHAR(255),CITY VARCHAR(255),INDUSTRY VARCHAR(255),NAME VARCHAR(255),STATE VARCHAR(255),ZIP VARCHAR(255))
CREATE MEMORY TABLE EMPLOYEE(EMPLOYEEID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,EMAIL VARCHAR(255),FIRSTNAME VARCHAR(255),LASTNAME VARCHAR(255),PHONE VARCHAR(255),TITLE VARCHAR(255),COMPANY_COMPANYID INTEGER,CONSTRAINT FK4AFD4ACEA791D268 FOREIGN KEY(COMPANY_COMPANYID) REFERENCES COMPANY(COMPANYID))
CREATE MEMORY TABLE PROGRAM$PLANNING(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,DATE TIMESTAMP,DESCRIPTION VARCHAR(255),PROGRAMID_ID INTEGER,CONSTRAINT FK8F1BA1BD3460F614 FOREIGN KEY(PROGRAMID_ID) REFERENCES PROGRAM(ID))
ALTER TABLE MUNICIPALITY ALTER COLUMN ID RESTART WITH 1
ALTER TABLE PROGRAM ALTER COLUMN ID RESTART WITH 1
ALTER TABLE COMPANY ALTER COLUMN COMPANYID RESTART WITH 1
ALTER TABLE EMPLOYEE ALTER COLUMN EMPLOYEEID RESTART WITH 1
ALTER TABLE PROGRAM$PLANNING ALTER COLUMN ID RESTART WITH 1
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
